define cICCount 4 # 0,1,2,3(,4,5)
alias rICCount r4
move rICCount cICCount
alias rCounter1 r0
alias rCounter2 r1 
alias rCommand r2
alias rRunning r3
alias rTemp1 r10
move rRunning 0
MainLoop:
l rTemp1 db Setting
breq rTemp1 rCommand 2
move rCommand rTemp1
beq rCommand 100 AllStop
beq rCommand 101 RunningLightStart
beq rCommand 102 FillingLightStart
j MainLoop
AllStop:
beqz rRunning MainLoop
move rCounter1 0
move rRunning 0
AllStopLoop:
s dr0 Setting 0
add rCounter1 rCounter1 1
ble rCounter1 rICCount AllStopLoop
j MainLoop
RunningLightStart:
move rCounter1 0
move rRunning 1
RunningLightLoop:
s dr0 Setting 1
s dr0 Setting 2
s dr0 Setting 4
s dr0 Setting 8
s dr0 Setting 16
s dr0 Setting 0
add rCounter1 rCounter1 1
ble rCounter1 rICCount RunningLightLoop
j MainLoop
FillingLightStart:
move rCounter1 0
move rRunning 1
FillingLightLoopOff:
s dr0 Setting 0
add rCounter1 rCounter1 1
ble rCounter1 rICCount FillingLightLoopOff
move rCounter1 0
FillingLightLoopOn:
s dr0 Setting 1
s dr0 Setting 3
s dr0 Setting 7
s dr0 Setting 15
s dr0 Setting 31
add rCounter1 rCounter1 1
ble rCounter1 rICCount FillingLightLoopOn
j MainLoop
